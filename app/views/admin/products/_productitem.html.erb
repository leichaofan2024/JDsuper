<tr>
  <td><%= product.image %></td>
  <td><%= link_to(product.title, admin_product_path(product)) %></td>
  <td><%= product.description %></td>
  <td><%= product.price %></td>
  <td><%= product.quantity %></td>
  <td><%= product.created_at %></td>
  <td><%= link_to("Edit", edit_admin_product_path(product), class: "btn btn-info")%>
      <%= link_to("Delete", admin_product_path(product), method: :delete, data: { :confirm => "Are you sure?" },class: "btn btn-danger")%>
  </td>
</tr>
