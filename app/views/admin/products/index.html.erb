<div class="col-md-10">
<div class="pull-right">
  <%= link_to("上新", new_admin_product_path, class: "btn btn-primary") %>
</div>
<table class="table table-bolder ">
  <thead>
    <tr>
      <th>#</th>
      <th>名称</th>
      <th>描述</th>
      <th>价格</th>
      <th>库存</th>
      <th>上新时间</th>
      <th>管理选项</th>
    </tr>
  </thead>
  <tbody>
    <%= render :partial => "productitem", :collection => @products, :as => :product %>
  </tbody>
</table>
</div>
