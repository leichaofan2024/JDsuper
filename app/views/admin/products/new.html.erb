<div class="col-md-10">
  <h3>上架新品</h3>
  <hr>
  <div class="" style="padding-top:50px">
    <%= simple_form_for [:admin,@product] do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :quantity %>

      <%= f.input :image, as: :file %>

      <%= f.submit "Submit", data: { disable_with: "Submiting..."} %>
    <% end %>
  </div>
</div>
